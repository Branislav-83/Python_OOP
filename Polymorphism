class Animal:
  def __init__(self, name):
    self.name = name

  def talk(self):
      pass
    

pet_qazi = Animal("Qazoo")
print(pet_qazi.name)  
print(pet_qazi.talk())

class Dog(Animal):
  def talk(self):
   return "Woof"

pet_dog = Dog("Dora")
print(pet_dog.name)
print(pet_dog.talk())  


class Cat(Animal):
  def talk(self):
    return "Mjau"

pet_cat = Cat("Cuki")
print(pet_cat.name)
print(pet_cat.talk())
